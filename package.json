{
  "name": "vue-demo",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "dev:firefox": "TARGET=firefox vite build --watch --mode development",
    "build": "vue-tsc --noEmit && vite build ",
    "build:firefox": "vue-tsc --noEmit && TARGET=firefox vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "type": "vue-tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@unocss/reset": "^0.50.6",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "colord": "^2.9.3",
    "lodash.debounce": "^4.0.8",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.29.7",
    "@vue/compiler-sfc": "^3.2.25",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.36.0",
    "jsdom": "^21.1.1",
    "less": "^4.1.3",
    "naive-ui": "^2.34.3",
    "taze": "^0.9.1",
    "typescript": "^5.0.2",
    "unocss": "^0.50.6",
    "unocss-preset-extra": "^0.5.0",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-icons": "^0.16.0",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.2",
    "vfonts": "^0.0.3",
    "vite": "^4.2.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-web-extension": "^3.0.2",
    "vitest": "^0.29.7",
    "vue": "^3.2.25",
    "vue-tsc": "^1.2.0",
    "webextension-polyfill": "^0.10.0"
  }
